#include <stdio.h>

int findRepeatedElement(int arr[], int n) {
    int xorAll = 0;


    for (int i = 0; i < n; i++) {
        xorAll ^= arr[i];
    }

    
    for (int i = 1; i < n; i++) {
        xorAll ^= i;
    }

    return xorAll;
}

int main() {
    int arr[] = {1, 3, 4, 2, 5, 3};
    int n = sizeof(arr) / sizeof(arr[0]);

    int repeated = findRepeatedElement(arr, n);
    printf("%d\n", repeated);

    return 0;
}
