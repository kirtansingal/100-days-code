#include <stdio.h>

struct Employee {
    char name[50];
    int id;
};

int main() {
    struct Employee e;
    FILE *f = fopen("emp.dat", "wb");
    scanf("%s %d", e.name, &e.id);
    fwrite(&e, sizeof(e), 1, f);
    fclose(f);
    
    f = fopen("emp.dat", "rb");
    fread(&e, sizeof(e), 1, f);
    printf("%s %d\n", e.name, e.id);
    fclose(f);
    return 0;
}
