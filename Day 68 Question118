#include <stdio.h>

int findMissingNumber(int arr[], int n) {
    int totalSum = n * (n + 1) / 2;  
    int arrSum = 0;

    for (int i = 0; i < n; i++) {
        arrSum += arr[i];
    }

    return totalSum - arrSum;
}

int main() {
    int arr[] = {0, 1, 2, 4, 5}; 
    int n = sizeof(arr) / sizeof(arr[0]);

    int missing = findMissingNumber(arr, n);
    printf("%d\n", missing);

    return 0;
}
