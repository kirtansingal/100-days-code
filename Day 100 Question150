#include <stdio.h>
#include <stdlib.h>

struct Student {
    char name[50];
    int roll_no;
    float marks;
};

int main() {
    struct Student *s = (struct Student*)malloc(sizeof(struct Student));
    
    scanf("%s %d %f", s->name, &s->roll_no, &s->marks);
    s->marks = 95.5;  
    
    printf("%s %d %.2f\n", s->name, s->roll_no, s->marks);
    
    free(s);
    return 0;
}
