#include <stdio.h>
#include <string.h>

int lengthOfLongestSubstring(char* s) {
    int n = strlen(s);
    int maxLen = 0;

    for (int i = 0; i < n; i++) {
        int visited[256] = {0}; 
        int length = 0;

        for (int j = i; j < n; j++) {
            if (visited[(unsigned char)s[j]] == 1) {
                break;
            } else {
                visited[(unsigned char)s[j]] = 1;
                length++;
            }
        }
        if (length > maxLen) {
            maxLen = length;
        }
    }

    return maxLen;
}

int main() {
    char s[1000];
    printf("Enter a string: ");
    fgets(s, sizeof(s), stdin);
    
    
    size_t len = strlen(s);
    if(len > 0 && s[len-1] == '\n') {
        s[len-1] = '\0';
    }

    int result = lengthOfLongestSubstring(s);
    printf("%d\n", result);

    return 0;
}
