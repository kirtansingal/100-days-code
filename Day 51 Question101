#include <stdio.h>

void findFirstLast(int nums[], int size, int target) {
    int first = -1, last = -1;
    for (int i = 0; i < size; i++) {
        if (nums[i] == target) {
            if (first == -1) first = i;
            last = i;
        }
    }
    printf("%d %d\n", first, last);
}

int main() {
    int nums[] = {1, 2, 2, 2, 3, 4, 5};
    int size = sizeof(nums) / sizeof(nums[0]);
    int target = 2;
    
    findFirstLast(nums, size, target);
    return 0;
}
