#include <stdio.h>

void sortArray(int arr[], int n) {
    // Simple bubble sort
    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < n - 1 - i; j++) {
            if (arr[j] > arr[j + 1]) {
                int temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }
}

int main() {
    int arr[] = {12, 3, 5, 7, 19};
    int n = sizeof(arr) / sizeof(arr[0]);
    int k = 3;

    sortArray(arr, n);

    if (k > 0 && k <= n) {
        printf("%d\n", arr[k - 1]);
    } else {
        printf("Invalid k\n");
    }

    return 0;
}
